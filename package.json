    "scripts": {
        "start": "node server.js",
        "debug": "node --inspect server.js",
        "dev": "nodemon",
        "start:global": "node server.js --global",
        "start:electron": "cd ./src/electron && npm run start",
        "start:deno": "deno run --allow-run --allow-net --allow-read --allow-write --allow-sys --allow-env server.js",
        "start:bun": "bun server.js",
        "start:no-csrf": "node server.js --disableCsrf",
        "postinstall": "node post-install.js",
        "tailwind:build": "tailwindcss -c tailwind.config.js -i public/css/st-tailwind.input.css -o public/css/st-tailwind.generated.css --minify",
        "lint": "eslint \"src/**/*.js\" \"public/**/*.js\" ./*.js",
        "lint:fix": "eslint \"src/**/*.js\" \"public/**/*.js\" ./*.js --fix",
        "plugins:update": "node plugins update",
        "plugins:install": "node plugins install"
    },
    "nodemonConfig": {
        "watch": [
            "src",
            "public",
            "server.js",
            "webpack.config.js"
        ],
        "ignore": [
            "data",
            "dist",
            "node_modules"
        ],
        "ext": "js,json,css,html",
        "exec": "node server.js"
    },
    "bin": {
        "routtavern": "./src/server-global.js"
    },
    "rules": {
        "no-path-concat": "off",
        "no-var": "off"
    },
    "main": "server.js",
